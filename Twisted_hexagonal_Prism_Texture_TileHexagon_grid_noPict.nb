        RowBox[{"1", ",", "0", ",", "0"}], "}"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"1", ",", 
       RowBox[{"{", 
        RowBox[{"0.5", ",", "0", ",", "0"}], "}"}]}], "}"}]}], "}"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Gradient2", "[", 
    RowBox[{"x_", ",", "grad_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"n", "=", 
        RowBox[{"Length", "[", "grad", "]"}]}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"i", "\[LessEqual]", "n"}], "&&", 
         RowBox[{
          RowBox[{"grad", "\[LeftDoubleBracket]", 
           RowBox[{"i", ",", "1"}], "\[RightDoubleBracket]"}], "<", "x"}]}], 
        ",", 
        RowBox[{"i", "++"}]}], "]"}], ";", 
      RowBox[{"RGBColor", "@@", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"1", "<", "i", "\[LessEqual]", "n"}], ",", 
         RowBox[{"Module", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"x1", "=", 
              RowBox[{"grad", "\[LeftDoubleBracket]", 
               RowBox[{
                RowBox[{"i", "-", "1"}], ",", "1"}], 
               "\[RightDoubleBracket]"}]}], ",", 
             RowBox[{"x2", "=", 
              RowBox[{"grad", "\[LeftDoubleBracket]", 
               RowBox[{"i", ",", "1"}], "\[RightDoubleBracket]"}]}]}], "}"}], 
           ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{"x2", "-", "x"}], ")"}], " ", 
               RowBox[{"grad", "\[LeftDoubleBracket]", 
                RowBox[{
                 RowBox[{"i", "-", "1"}], ",", "2"}], 
                "\[RightDoubleBracket]"}]}], "+", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"x", "-", "x1"}], ")"}], 
               RowBox[{"grad", "\[LeftDoubleBracket]", 
                RowBox[{"i", ",", "2"}], "\[RightDoubleBracket]"}]}]}], ")"}],
             "/", 
            RowBox[{"(", 
             RowBox[{"x2", "-", "x1"}], ")"}]}]}], "]"}], ",", 
         RowBox[{"grad", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{"Min", "[", 
            RowBox[{"i", ",", "n"}], "]"}], ",", "2"}], 
          "\[RightDoubleBracket]"}]}], "]"}]}]}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"k", "[", "t_", "]"}], "=", 
   RowBox[{"Max", "[", 
    RowBox[{
     RowBox[{"Abs", "[", 
      RowBox[{"Cos", "[", "t", "]"}], "]"}], ",", 
     RowBox[{"Abs", "[", 
      RowBox[{"Cos", "[", 
       RowBox[{"t", "+", 
        RowBox[{"2", "*", 
         RowBox[{"Pi", "/", "3"}]}]}], "]"}], "]"}], ",", 
     RowBox[{"Abs", "[", 
      RowBox[{"Cos", "[", 
       RowBox[{"t", "-", 
        RowBox[{"2", "*", 
         RowBox[{"Pi", "/", "3"}]}]}], "]"}], "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "t_", "]"}], "=", 
   RowBox[{
    RowBox[{"Cos", "[", "t", "]"}], "/", " ", 
    RowBox[{"k", "[", "t", "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"g", "[", "t_", "]"}], "=", 
   RowBox[{
    RowBox[{"Sin", "[", "t", "]"}], "/", " ", 
    RowBox[{"k", "[", "t", "]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"n0", "=", "3"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"t0", "=", 
