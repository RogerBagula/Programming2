Cell[CellGroupData[{

Cell["\<\
(*  p is the parameter   *)

Clear[f,x,firstIt,iterates,seed,pmin,pmax,p,
\tpsteps,step,bifurcList,data];

f = Compile[{{x,_Real},{p,_Real}}, (2 x^3 - 1)/(3x^2 + p)];

(* firstIt is the first iteration shown, 
\titerates is the maximum number of iterations 
\t\tcalculated after firstIt,
\tseed is the starting point of the iteration. *)

firstIt = 500;
iterates = 150;
seed = 0.0;

(*  pmin and pmax define the bounds of the parameters,
\t\tpsteps is the number of paramter values used.  *)

pmin =  -1.3;
pmax =  -1.25;
psteps = 1000;

p = N[pmin];
step = N[(pmax - pmin)/psteps];

bifurcList[p_] := Module[
\t{startPoint = Nest[f[#,p]&,seed,firstIt]},
\tMap[{p,#}&,
\t\t\tFixedPointList[f[#,p]&,
\t\t\t\tstartPoint,
\t\t\t\titerates,
\t\t\t\tSameTest -> (#2 == startPoint &)]]]

Timing[
data = \tPartition[
\t\t\tFlatten[
\t\t\t\tTable[
\t\t\t\t\tbifurcList[t],
\t\t\t\t\t{t,pmin,pmax,N[(pmax-pmin)/psteps]}]
\t\t\t],
\t\t2];
]

Length[data]\
\>", "Input",
