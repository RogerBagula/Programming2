Cell["\<\
Because this generator is parabolic we get kissing circles. In order to get a \
reasonable approximation of the limit set the separation lone must be modified.
ta=1.642139 - .766588I\
\>", "Text",ExpressionUUID->"5a27d6a8-ccc4-436c-a4fc-63f6dfdb1351"],

Cell[BoxData[
 RowBox[{
  RowBox[{"kleinC", "=", 
   RowBox[{"Compile", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{"z", ",", "_Complex"}], "}"}], "}"}], ",", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"t", "=", 
          RowBox[{"1.642139", "-", 
           RowBox[{".766588", "I"}]}]}], ",", "u", ",", "v", ",", "xi", ",", 
         "yi", ",", 
         RowBox[{"zi", "=", 
          RowBox[{"(", 
           RowBox[{"1.", "+", "I"}], ")"}]}], ",", 
         RowBox[{"k", "=", "1"}], ",", 
         RowBox[{"low", "=", 
          RowBox[{"1", "+", "I"}]}], ",", 
         RowBox[{"high", "=", 
          RowBox[{"1", "+", "I"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"xi", "=", 
         RowBox[{"Re", "[", "z", "]"}]}], ";", " ", 
        RowBox[{"yi", "=", 
         RowBox[{"Im", "[", "z", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"u", "=", 
         RowBox[{"Re", "[", "t", "]"}]}], ";", " ", 
        RowBox[{"v", "=", 
         RowBox[{"Im", "[", "t", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"yi", "<", "0"}], "||", 
           RowBox[{"yi", ">", "u"}]}], ",", 
          RowBox[{"Return", "[", "0", "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"xi", "=", 
            RowBox[{
             RowBox[{"Mod", "[", 
              RowBox[{
               RowBox[{"xi", "+", "1.", "+", 
                RowBox[{"v", "  ", 
                 RowBox[{"yi", "/", "u"}]}]}], ",", "2"}], "]"}], "-", "1", 
             "-", 
             RowBox[{"v", " ", 
              RowBox[{"yi", "/", "u"}]}]}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"zi", "=", 
            RowBox[{"xi", "+", 
             RowBox[{"I", " ", "yi"}]}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"(*", 
            RowBox[{"check", " ", "separation", " ", "line"}], "*)"}], 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"yi", "<", 
              RowBox[{
               RowBox[{"u", "/", "2."}], "+", 
               RowBox[{
                RowBox[{"Sign", "[", 
                 RowBox[{"xi", "+", 
                  RowBox[{"v", "/", "2"}]}], "]"}], "0.02", "*", "u", 
                RowBox[{"(", 
                 RowBox[{"2", "-", 
                  RowBox[{"Abs", "@", 
                   RowBox[{"Exp", "[", 
                    RowBox[{"4.5", " ", 
                    RowBox[{"Abs", "[", 
                    RowBox[{"xi", "+", 
                    RowBox[{"v", "/", "2."}]}], "]"}]}], "]"}]}]}], 
                 ")"}]}]}]}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Abs", "[", 
                  RowBox[{"zi", "-", "low"}], "]"}], "<", 
                 RowBox[{"10", "^", 
                  RowBox[{"-", "4"}]}]}], " ", ",", 
                RowBox[{
                 RowBox[{"k", "=", 
                  RowBox[{"-", "n"}]}], ";", 
                 RowBox[{"Break", "[", "]"}]}], ",", 
                RowBox[{"low", "=", "zi"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"k", "=", "1"}], ";", "\[IndentingNewLine]", 
              RowBox[{"zi", "=", 
               RowBox[{
                RowBox[{"I", "*", "t"}], "+", 
                RowBox[{"1.", "/", "zi"}]}]}]}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Abs", "[", 
                  RowBox[{"zi", "-", "high"}], "]"}], "<", 
                 RowBox[{"10", "^", 
                  RowBox[{"-", "4"}]}]}], " ", ",", 
                RowBox[{
                 RowBox[{"k", "=", 
                  RowBox[{"-", "n"}]}], ";", 
                 RowBox[{"Break", "[", "]"}]}], ",", 
                RowBox[{"high", "=", "zi"}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"zi", "=", 
               RowBox[{"I", "/", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"I", " ", "*", "zi"}], "+", "t"}], ")"}]}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"k", "=", "2"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
           
           RowBox[{"xi", "=", 
            RowBox[{"Re", "[", "zi", "]"}]}], ";", " ", 
           RowBox[{"yi", "=", 
            RowBox[{"Im", "[", "zi", "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"yi", "<", "0"}], "||", 
              RowBox[{"yi", ">", "u"}]}], " ", ",", " ", 
             RowBox[{"Break", "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"k", "=", "3"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"n", ",", "1", ",", "30"}], "}"}]}], "\[IndentingNewLine]",
          "]"}], ";", "\[IndentingNewLine]", "k"}]}], "]"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellLabel->"In[32]:=",ExpressionUUID->"ee7eaf1f-e9af-4745-91f9-2cd80837ddd9"],

