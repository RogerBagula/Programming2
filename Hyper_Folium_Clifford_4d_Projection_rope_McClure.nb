Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"two", " ", "tubes", " ", "programs", " ", "for", " ", 
    RowBox[{"knots", ":", " ", "parametrics"}]}], "*)"}], "\n", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Hidekazu", " ", "Takahashi"}], "\n", "<", "Mail", ">", 
         "hidek"}], "..."}], "@", "pop"}], ".", "biwako", ".", "ne", ".", 
      "jp"}], "\n", "<", "URL", ">", 
     RowBox[{"http", ":"}]}], "//", 
    RowBox[{
     RowBox[{
      RowBox[{"www", ".", "biwako", ".", "ne", ".", "jp"}], "/", 
      RowBox[{"~", "hidekazu"}]}], "/"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Clear", "[", "rope", "]"}], "\[IndentingNewLine]", 
   RowBox[{"Off", "[", 
    RowBox[{"ParametricPlot3D", "::", "\"\<ppcom\>\""}], "]"}], "\n", 
   RowBox[{
    RowBox[{"color", "[", "n_", "]"}], ":=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "Blue", ",", "Red", ",", "Yellow", ",", "Green", ",", "Magenta", ",", 
       "Orange", ",", "Purple"}], "}"}], "[", 
     RowBox[{"[", 
      RowBox[{"n", "+", "1"}], "]"}], "]"}]}], "\n", 
   RowBox[{
    RowBox[{"al", "[", 
     RowBox[{"{", 
      RowBox[{"a_", ",", "b_", ",", "c_"}], "}"}], "]"}], ":=", 
    RowBox[{"N", "[", 
     RowBox[{"-", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"a", "\[Equal]", "0"}], "&&", 
         RowBox[{"b", "\[Equal]", "0"}]}], ",", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"c", ">", "0"}], ",", 
          RowBox[{"Pi", "/", "2"}], ",", 
          RowBox[{"-", 
           RowBox[{"(", 
            RowBox[{"Pi", "/", "2"}], ")"}]}]}], "]"}], ",", 
        RowBox[{"ArcTan", "[", 
         RowBox[{"c", "/", 
          RowBox[{"Sqrt", "[", 
           RowBox[{
            RowBox[{"a", "^", "2"}], "+", 
            RowBox[{"b", "^", "2"}]}], "]"}]}], "]"}]}], "]"}]}], "]"}]}], 
   "\n", 
   RowBox[{
    RowBox[{"be", "[", 
     RowBox[{"{", 
      RowBox[{"a_", ",", "b_", ",", "c_"}], "}"}], "]"}], ":=", 
    RowBox[{"N", "[", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", "d", "}"}], ",", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"a", "\[Equal]", "0"}], ",", 
         RowBox[{"Which", "[", 
          RowBox[{
           RowBox[{"b", ">", "0"}], ",", 
           RowBox[{"Pi", "/", "2"}], ",", 
           RowBox[{"b", "\[Equal]", "0"}], ",", "0", ",", 
           RowBox[{"b", "<", "0"}], ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"3", "Pi"}], ")"}], "/", "2"}]}], "]"}], ",", 
         RowBox[{
          RowBox[{"d", "=", 
           RowBox[{"ArcTan", "[", 
            RowBox[{"b", "/", "a"}], "]"}]}], ";", 
          RowBox[{"Which", "[", 
           RowBox[{
            RowBox[{"a", "<", "0"}], ",", 
            RowBox[{"d", "+", "Pi"}], ",", 
            RowBox[{
             RowBox[{"a", ">", "0"}], "&&", 
             RowBox[{"b", "\[GreaterEqual]", "0"}]}], ",", "d", ",", 
            RowBox[{
             RowBox[{"a", ">", "0"}], "&&", 
             RowBox[{"b", "<", "0"}]}], ",", 
            RowBox[{"d", "+", 
             RowBox[{"2", "Pi"}]}]}], "]"}]}]}], "]"}]}], "]"}], "]"}]}], 
   "\n", 
   RowBox[{
    RowBox[{"poi", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a_", ",", "b_", ",", "c_"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"x1_", ",", "y1_", ",", "z1_"}], "}"}], ",", "r_", ",", "t_"}],
      "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"aa", ",", "bb", ",", "ry", ",", "rz"}], "}"}], ",", 
      RowBox[{
       RowBox[{"aa", "=", 
        RowBox[{"al", "[", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b", ",", "c"}], "}"}], "]"}]}], ";", 
       RowBox[{"bb", "=", 
        RowBox[{"be", "[", 
         RowBox[{"{", 
          RowBox[{"a", ",", "b", ",", "c"}], "}"}], "]"}]}], ";", 
       RowBox[{"ry", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Cos", "[", "aa", "]"}], ",", "0", ",", 
            RowBox[{"Sin", "[", "aa", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "1", ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"Sin", "[", "aa", "]"}]}], ",", "0", ",", 
            RowBox[{"Cos", "[", "aa", "]"}]}], "}"}]}], "}"}]}], ";", 
       RowBox[{"rz", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Cos", "[", "bb", "]"}], ",", 
            RowBox[{"-", 
             RowBox[{"Sin", "[", "bb", "]"}]}], ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Sin", "[", "bb", "]"}], ",", 
            RowBox[{"Cos", "[", "bb", "]"}], ",", "0"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "}"}]}], ";", 
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"r", " ", 
          RowBox[{"rz", ".", "ry", ".", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"Sin", "[", "t", "]"}], ",", 
             RowBox[{"Cos", "[", "t", "]"}]}], "}"}]}]}], "+", 
         RowBox[{"{", 
          RowBox[{"x1", ",", "y1", ",", "z1"}], "}"}]}], "]"}]}]}], "]"}]}], 
   "\n", 
   RowBox[{
    RowBox[{"rope", "[", 
     RowBox[{"c_", ",", "h_", ",", "r_", ",", "opts___"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"vec", ",", "s"}], "}"}], ",", 
      RowBox[{
       RowBox[{"vec", "=", 
        RowBox[{"D", "[", 
         RowBox[{"c", ",", 
          RowBox[{"First", "[", "h", "]"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ParametricPlot3D", "[", 
        RowBox[{
         RowBox[{"poi", "[", 
          RowBox[{"vec", ",", "c", ",", "r", ",", "s"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"s", ",", "0", ",", 
           RowBox[{"2", "*", "Pi"}]}], "}"}], ",", "h", ",", 
         RowBox[{"Boxed", "\[Rule]", "False"}], ",", 
         RowBox[{"Axes", "\[Rule]", "False"}], ",", 
         RowBox[{"Mesh", "\[Rule]", "False"}], ",", 
         RowBox[{"PlotStyle", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"LightBlue", ",", 
            RowBox[{"Specularity", "[", 
             RowBox[{"White", ",", "10"}], "]"}]}], "}"}]}], ",", "opts"}], 
        "]"}]}]}], "]"}]}]}]}]], "Input",
